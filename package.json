{
  "name": "foxios",
  "version": "1.0.13",
  "description": "A TypeScript wrapper fetch wrapper with interceptors and strong typing.",
  "main": "dist/foxios.min.js",
  "module": "dist/foxios.min.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:tsc": "tsc --declaration --declarationDir dist/types --outDir dist src/index.ts",
    "build:esbuild": "esbuild src/core/foxios.ts --bundle --minify --sourcemap --platform=node --target=esnext --outfile=dist/foxios.min.js",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "ts-node": {
    "esm": true
  },
  "keywords": [
    "fetch",
    "typescript",
    "http",
    "api",
    "interceptors"
  ],
  "author": "Kaleb",
  "license": "MIT",
  "files": [
    "dist/foxios.min.js",
    "dist/foxios.min.mjs",
    "dist/types/index.d.ts",
    "dist/types/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kaleb110/foxios.git"
  },
  "bugs": {
    "url": "https://github.com/kaleb110/foxios/issues"
  },
  "homepage": "https://github.com/kaleb110/foxios#readme",
  "devDependencies": {
    "esbuild": "^0.25.0",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^4.0.0",
    "vitest": "^3.0.7"
  }
}
